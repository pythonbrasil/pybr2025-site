<?xml version="1.0" encoding="utf-8"?>
<object xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        meta_type="Dexterity FTI"
        name="SponsorsDB"
        i18n:domain="pythonbrasil.site"
>

  <!-- Basic properties -->
  <property name="title"
            i18n:translate=""
  >Sponsors Database</property>
  <property name="description"
            i18n:translate=""
  >Conference Sponsoring information.</property>

  <property name="allow_discussion">False</property>
  <property name="factory">SponsorsDB</property>
  <property name="icon_expr" />
  <property name="link_target" />

  <!-- Hierarchy control -->
  <property name="global_allow">True</property>
  <property name="filter_content_types">True</property>
  <property name="allowed_content_types">
    <element value="Document" />
    <element value="File" />
    <element value="SponsorLevel" />
  </property>
  <!-- Schema, class and security -->
  <property name="add_permission">pythonbrasil.site.AddSponsorsDB</property>
  <property name="klass">pythonbrasil.site.content.sponsors.sponsors_db.SponsorsDB</property>
  <property name="model_file" />
  <property name="model_source" />
  <property name="schema">pythonbrasil.site.content.sponsors.sponsors_db.ISponsorsDB</property>

  <!-- Enabled behaviors -->
  <property name="behaviors"
            purge="false"
  >
    <element value="plone.dublincore" />
    <element value="volto.preview_image" />
    <element value="volto.navtitle" />
    <element value="volto.blocks" />
    <element value="plone.namefromtitle" />
    <element value="plone.excludefromnavigation" />
    <element value="plone.shortname" />
    <element value="volto.navtitle" />
  </property>

</object>
